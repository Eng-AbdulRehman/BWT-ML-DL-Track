    "def Is_Even(Number):\n",
    "    if Number % 2 == 0:\n",
    "        print(f\"The Number {Number} is Even Number .\")\n",
    "        return True\n",
    "    else:\n",
    "        print(f\"The Number {Number} is odd Number.\")\n",
    "        return False\n",
    "N = int(input(\"Enter the Integer: \"))\n",
    "Is_Even(N)"
